{
	"name": "newNavigation",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"build:clean": "rm -Rf ios/Pods ios/Podfile.lock android/.idea android/.gradle ios/build android/build android/app/build",
		"dev:refresh": "rm -Rf node_modules/ && yarn cache clean && yarn && yarn build:clean",
